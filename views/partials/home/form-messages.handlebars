{{#if successMessage}}
    {{#each successMessage}}
        <div class="alert alert-success">
            {{this}}
        </div>
    {{/each}}
{{/if}}

{{#if errorMessage}}
    {{#each errorMessage}}
        {{!-- {{flashArrayMessages}} --}}
        <div class="alert alert-danger">
            {{this}}
        </div>
    {{/each}}
{{/if}}

{{#if successFlash}}
    {{#each successFlash}}
        <div class="alert alert-success">
            {{this}}
        </div>
    {{/each}}
{{/if}}

{{#if failureFlash}}
    {{#each failureFlash}}
        <div class="alert alert-danger">
            {{this}}
        </div>
    {{/each}}
{{/if}}